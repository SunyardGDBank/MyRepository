<?xml version="1.0" encoding="UTF-8"?>
<screen xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://moqui.org/xsd/xml-screen-2.1.xsd"
        default-menu-title="路由明细" default-menu-include="false">
    <parameter name="mrchntNo"/>
    <parameter name="mrchntNm"/>
    <parameter name="mrchntSeqId"/>
    <parameter name="version"/>
    <transition name="createBlacklistTab">
        <service-call name="nacs.route.admin.NacsRouteService.create#BlacklistTab"/>
        <default-response url="."/>
    </transition>
    <transition name="updateBlacklistTab">
        <service-call name="update#nacs.para.BlacklistTab"/>
        <default-response url="."/>
    </transition>
    <transition name="createWhitelistTab">
        <service-call name="nacs.route.admin.NacsRouteService.create#WhitelistTab"/>
        <default-response url="."/>
    </transition>
    <transition name="createMrchntRoute">
        <service-call name="nacs.route.admin.NacsRouteService.create#MrchntRoute"/>
        <default-response url="."/>
    </transition>
    <transition name="createMrchntRouteCo">
        <service-call name="nacs.route.admin.NacsRouteService.create#MrchntRouteCo"/>
        <default-response url="."/>
    </transition>
    <transition name="updateWhitelistTab">
        <service-call name="update#nacs.para.WhitelistTab"/>
        <default-response url="."/>
    </transition>
    <transition name="updateKnowledgeBaseSet">
        <service-call name="update#nacs.para.KnowledgeBaseSet"/>
        <default-response url="."/>
    </transition>
    <transition name="updateMrchntRoute">
        <service-call name="nacs.route.admin.NacsRouteService.update#MrchntRoute" in-map="context"/>
        <default-response url="."/>
    </transition>
    <transition name="updateMrchntRouteCo">
        <service-call name="nacs.route.admin.NacsRouteService.update#MrchntRouteCo" in-map="context"/>
        <default-response url="."/>
    </transition>
    <actions>
        <entity-find entity-name="nacs.mrchnt.Mrchnt" list="routeList">
            <econdition field-name="mrchntNo" from="mrchntNo"/>
        </entity-find>
        <entity-find entity-name="nacs.para.BlacklistTab" list="blacklistTabList">
            <econdition field-name="mrchntSeqId" from="mrchntSeqId"/>
            <order-by field-name="seqId"/>
        </entity-find>
        <entity-find entity-name="nacs.para.WhitelistTab" list="whitelistTabList">
            <econdition field-name="mrchntSeqId" from="mrchntSeqId"/>
            <order-by field-name="seqId"/>
        </entity-find>
        <entity-find entity-name="nacs.para.KnowledgeBaseSet" list="knowledgeBaseSetList">
            <econdition field-name="mrchntSeqId" from="mrchntSeqId"/>
            <order-by field-name="nacsTxId"/>
        </entity-find>
        <entity-find entity-name="moqui.basic.Enumeration" list="trxCtgyList">
            <econdition field-name="enumTypeId" value="TrxCtgy"/>
        </entity-find>
        <entity-find entity-name="moqui.basic.Enumeration" list="takeEffectFlgList">
            <econdition field-name="enumTypeId" value="TakeEffectFlg"/>
        </entity-find>
        <entity-find entity-name="moqui.basic.Enumeration" list="matchTypeList">
            <econdition field-name="enumTypeId" value="MatchType"/>
        </entity-find>
        <entity-find entity-name="moqui.basic.Enumeration" list="opnFlgList">
            <econdition field-name="enumTypeId" value="OpnFlg"/>
        </entity-find>
        <entity-find entity-name="nacs.channel.ChannelView" list="channelSeqViewList"/>
        <set field="mmm" from="trxCtgy" default-value="000401"/>
        <set field="nnn" from="trxCtgy" default-value="000501"/>
        <entity-find entity-name="nacs.mrchnt.MrchntRoute" list="mrchntRouteList">
            <econdition field-name="mrchntSeqId" from="mrchntSeqId"/>
            <econdition field-name="trxCtgy" value="000401"/>
            <order-by field-name="seqId"/>
        </entity-find>
        <entity-find entity-name="nacs.mrchnt.MrchntRoute" list="mrchntRouteCoList">
            <econdition field-name="mrchntSeqId" from="mrchntSeqId"/>
            <econdition field-name="trxCtgy" value="000501"/>
            <order-by field-name="seqId"/>
        </entity-find>

        <log level="info" message="mrchntRouteList##########${mrchntRouteList}"/>
    </actions>
    <widgets>
        <!--页面顶部显示的信息-->
        <form-single name="UserGroupForm" map="napPayOrgList">
            <field name="mrchntNo"><default-field title="商户编号"><display/></default-field></field>
            <field name="mrchntNm"><default-field title="商户名称"><display/></default-field></field>
            <field name="version"><default-field title="协议版本"><display/></default-field></field>
            <field-layout>
                <field-row-sm col-sm="4">
                    <field-ref name="mrchntNo"/>
                    <field-ref name="mrchntNm"/>
                    <field-ref name="version"/>
                </field-row-sm>
            </field-layout>
        </form-single>
        <form-list name="ListNacsRoute" list="routeList">
            <field name="seqId">
                <default-field title="序号">
                    <hidden/>
                </default-field>
            </field>
            <field name="mrchntSeqId" from="seqId">
                <default-field>
                    <hidden/>
                </default-field>
            </field>
            <field name="mrchntFlag" align="center">
                <default-field title="商户启用标志">
                    <display-entity entity-name="moqui.basic.Enumeration" key-field-name="enumId"
                                    text="${description}"/>
                </default-field>
            </field>
            <field name="mrchntBeginTime" align="center">
                <default-field title="运行开启时间">
                    <display/>
                </default-field>
            </field>
            <field name="mrchntEndTime" align="center">
                <default-field title="运行关闭时间">
                    <display/>
                </default-field>
            </field>
