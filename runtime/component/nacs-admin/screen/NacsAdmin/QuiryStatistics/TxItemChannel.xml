<screen xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://moqui.org/xsd/xml-screen-2.1.xsd"
        default-menu-title="支付通道流水" default-menu-index="2" >
    <!--支付通道流水查询统计-->


    <parameter name="nacsTxId"/>
    <parameter name="workDate_from"/>

    <transition name="findTxItemChannel">
        <default-response url="."/>
    </transition>

    <actions>

        <if condition="!workDate_from">
            <set field="workDate_from" value="${new Date().format('yyyy-MM-dd')}"/>
        </if>

        <entity-find entity-name="nacs.trx.TxItemChannel" list="TxItemChannelList">
            <search-form-inputs/>
            <order-by field-name="-workDate"/>
        </entity-find>

        <!--交易类别-->
        <entity-find entity-name="moqui.basic.Enumeration" list="trxCtgyList">
            <econdition field-name="enumTypeId" value="TrxCtgy"/>
        </entity-find>

        <!--平台处理状态-->
        <entity-find entity-name="moqui.basic.StatusItem" list="txStatuIdList">
            <econdition field-name="statusTypeId" value="TxStatuId"/>
        </entity-find>

        <!--支付通道编号-->
        <entity-find entity-name="nacs.channel.ChannelView" list="channelViewList" />

        <!--商户参数表-->
        <entity-find entity-name="nacs.mrchnt.MrchntView" list="mrchntViewList"/>

        <!--证件类型-->
        <entity-find entity-name="moqui.basic.Enumeration" list="certcodeList">
            <econdition field-name="enumTypeId" value="Certcode"/>
        </entity-find>
        <!--支付通道状态-->
        <entity-find entity-name="moqui.basic.StatusItem" list="payStatusList">
            <econdition field-name="statusTypeId" value="PayStatus"/>
        </entity-find>

        <!--处理状态-->
        <entity-find entity-name="moqui.basic.StatusItem" list="tranStasList">
            <econdition field-name="statusTypeId" value="TranStas"/>
        </entity-find>

        <!--子商户-->
        <entity-find entity-name="nacs.mrchnt.SubMrchnt" list="subMrchntList"/>
    </actions>


    <widgets>
        <container-dialog id="findTxItemChannel" button-text="查找" width="1350">
            <form-single name="FindTxItemChannel" transition="findTxItemChannel">
                <!--平台流水号-->
                <field name="nacsTxId">
                    <default-field title="平台流水号"><text-find hide-options="true"/></default-field>
                </field>

                <!--商户编号-->
                <field name="mrchntSeqId">
                    <default-field title="商户编号">
                        <drop-down allow-empty="true">
                            <entity-options key="${seqId}" text="${mrchntNo} - ${mrchntNm}">
                                <entity-find entity-name="nacs.mrchnt.Mrchnt"/>
                            </entity-options>
                        </drop-down>
                    </default-field>
                </field>

                <!--支付通道编号-->
                <field name="channelSeqId">
                    <default-field title="支付通道编号">
                        <drop-down allow-empty="true">
                            <entity-options key="${seqId}" text="${channelNo} - ${channelNm}">
                                <entity-find entity-name="nacs.channel.Channel"/>
                            </entity-options>
                        </drop-down>
                    </default-field>
                </field>

                <!--工作日期区间-->
                <field name="workDate">
                    <default-field title="工作日期区间"><date-find type="date"/></default-field>
                </field>

                <!--对账日期区间-->
                <field name="checkDate">
                    <default-field title="对账日期"><date-time type="date"/></default-field>
                </field>

                <!--订单发送时间-->
                <field name="txnTime">
                    <default-field title="订单发送时间"><date-time type="date"/></default-field>
                </field>

                <!--商户流水号-->
                <field name="buiTxId">
                    <default-field title="商户流水号"><text-find hide-options="true"/></default-field>
                </field>

                <!--支付通道流水号-->
                <field name="payTxId">
                    <default-field title="支付通道流水号"><text-find hide-options="true"/></default-field>
                </field>

                <!--订单号-->
                <field name="orderId">
                    <default-field title="订单号"><text-find hide-options="true"/></default-field>
                </field>

                <!--交易类别-->
                <field name="trxCtgy">
                    <default-field title="交易类别">
                        <drop-down allow-empty="true">
                            <list-options list="trxCtgyList" key="${enumId}" text="${description}"/>
                        </drop-down>
                    </default-field>
                </field>

                <!--支付通道状态-->
                <field name="payStatus">
                    <default-field title="支付通道状态">
                        <drop-down allow-empty="true">
                            <list-options list="payStatusList" key="${statusId}" text="${description}"/>
                        </drop-down>
                    </default-field>
                </field>

                <!--子商户号-->
                <field name="subMrchntNo">
                    <default-field title="子商户编号">
                        <drop-down allow-empty="true">
                            <list-options list="subMrchntList" key="${subMrchntNo}" text="${subMrchntNo} - ${subMrchntNm}"/>
                        </drop-down>
                    </default-field>
                </field>

                <!--处理状态-->
                <field name="tranStas">
                    <default-field title="处理状态">
                        <drop-down allow-empty="true">
                            <list-options list="tranStasList" key="${statusId}" text="${description}"/>
                        </drop-down>
                    </default-field>
                </field>

                <!--平台处理状态-->
                <field name="txStatuId">
                    <default-field title="平台处理状态">
                        <drop-down allow-empty="true">
                            <list-options list="txStatuIdList" key="${statusId}" text="${description}"/>
                        </drop-down>
                    </default-field>
                </field>

                <!--交易金额区间-->
                <field name="trxAmt">
                    <default-field title="交易金额区间"><range-find/></default-field>
                </field>

                <!--收款人账号-->
                <field name="payeeAcctno">
                    <default-field title="收款人账号"><text-find hide-options="true"/></default-field>
                </field>

                <!--收款人姓名-->
                <field name="payeeAcctName">
                    <default-field title="收款人姓名"><text-find hide-options="true"/></default-field>
                </field>

                <!--付款人账号-->
                <field name="payerAcctno">
                    <default-field title="付款人账号"><text-find hide-options="true"/></default-field>
                </field>

                <!--付款人姓名-->
                <field name="payerAcctName">
                    <default-field title="付款人姓名"><text-find hide-options="true"/></default-field>
                </field>

                <!--证件类型-->
                <field name="certCode">
                    <default-field title="证件类型">
                        <drop-down allow-empty="true">
                            <list-options list="certcodeList" key="${enumId}" text="${description}"/>
                        </drop-down>
                    </default-field>
                </field>

                <!--证件号-->
                <field name="certNo">
                    <default-field title="证件号"><text-find hide-options="true"/></default-field>
                </field>

                <!--查找按钮-->
                <field name="searchButton">
                    <default-field title="Find"><submit icon="glyphicon glyphicon-search"/></default-field>
                </field>

                <!--布局-->
                <field-layout>
                    <field-row>
                        <field-ref name="nacsTxId"/>
                        <field-ref name="mrchntSeqId"/>
                    </field-row>
                    <field-row>
                        <field-ref name="channelSeqId"/>
                        <field-ref name="workDate"/>
                    </field-row>
                    <field-row>
                        <field-ref name="checkDate"/>
                        <field-ref name="txnTime"/>
                    </field-row>
                    <field-row>
                        <field-ref name="buiTxId"/>
                        <field-ref name="payTxId"/>
                    </field-row>
                    <field-row>
                        <field-ref name="orderId"/>
                        <field-ref name="trxCtgy"/>
                    </field-row>
                    <field-row>
                        <field-ref name="payStatus"/>
                        <field-ref name="subMrchntNo"/>
                    </field-row>
                    <field-row>
                        <field-ref name="tranStas"/>
                        <field-ref name="txStatuId"/>
                    </field-row>
                    <field-row>
                        <field-ref name="trxAmt"/>
                        <field-ref name="payeeAcctno"/>
                    </field-row>
                    <field-row>
                        <field-ref name="payeeAcctName"/>
                        <field-ref name="payerAcctno"/>
                    </field-row>
                    <field-row>
                        <field-ref name="payerAcctName"/>
                        <field-ref name="certCode"/>
                    </field-row>
                    <field-row>
                        <field-ref name="certNo"/>
                        <field-ref name="searchButton"/>
                    </field-row>
                </field-layout>
            </form-single>
        </container-dialog>

        <field name="return">
            <default-field>
                <link url="." text="返回"/>
            </default-field>
        </field>

