    <!--接入商户管理：商户列表,商户明细-->

    <!--默认界面:商户列表-->
    <subscreens always-use-full-path="true" default-item="MrchntList">
        <subscreens-item name="MrchntList" location="component://nacs-mrchnt/screen/NacsMrchnt/MrchntList.xml"
                         menu-index="1"/>
        <subscreens-item name="MrchntDetail" location="component://nacs-mrchnt/screen/NacsMrchnt/MrchntDetail.xml"
