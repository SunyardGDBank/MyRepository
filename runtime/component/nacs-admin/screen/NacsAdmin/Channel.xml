<screen xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://moqui.org/xsd/xml-screen-2.1.xsd"
        default-menu-title="通道管理" menu-image="glyphicon glyphicon-road" menu-image-type="icon" default-menu-index="4">
    <!--通道管理：支付通道列表,支付通道明细-->

    <!--默认界面:支付通道列表-->
    <subscreens always-use-full-path="true" default-item="ChannelList">
        <subscreens-item name="ChannelList" location="component://nacs-channel/screen/NacsChannel/ChannelList.xml"
                         menu-index="1"/>
        <subscreens-item name="ChannelDetail" location="component://nacs-channel/screen/NacsChannel/ChannelDetail.xml"
                         menu-include="false"/>
    </subscreens>
    <widgets>
        <subscreens-panel id="ChannePanel" type="popup"/>
    </widgets>
</screen>
